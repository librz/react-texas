{
  "name": "react-texas",
  "version": "0.2.1",
  "description": "React text ellipsis component",
  "author": "librz",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/Ellipsis.d.ts",
  "keywords": [
    "react",
    "ellipsis",
    "text",
    "texas"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/librz/react-texas.git"
  },
  "bugs": {
    "url": "https://github.com/librz/react-texas/issues"
  },
  "homepage": "https://github.com/librz/react-texas#readme",
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && tsc --emitDeclarationOnly && NODE_ENV=production babel src/lib --copy-files --out-dir dist --extensions=.js,.jsx,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 ||  || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  }
}
